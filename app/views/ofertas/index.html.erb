<h1>Listing ofertas</h1>

<table>
  <tr>
    <th>Titulo</th>
    <th>Descricao</th>
    <th>Data</th>
    <th>Preco de</th>
    <th>Preco por</th>
    <th>Parceiro</th>
    <th>Ativo</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @ofertas.each do |oferta| %>
  <tr>
    <td><%= oferta.titulo %></td>
    <td><%= oferta.descricao %></td>
    <td><%= oferta.data %></td>
    <td><%= oferta.preco_de %></td>
    <td><%= oferta.preco_por %></td>
    <td><%= oferta.parceiro_id %></td>
    <td><%= oferta.ativo %></td>
    <td><%= link_to 'Show', oferta %></td>
    <td><%= link_to 'Edit', edit_oferta_path(oferta) %></td>
    <td><%= link_to 'Destroy', oferta, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Oferta', new_oferta_path %>
