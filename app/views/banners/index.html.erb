<h1>Listing banners</h1>

<table>
  <tr>
	<th></th>
    <th>Url</th>
    <th>Path</th>
    <th>Parceiro</th>
    <th>Data</th>
    <th>Ativo</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @banners.each do |banner| %>
  <tr>
	<td><%= image_tag banner.imagem.url(:thumb) %></td>
    <td><%= banner.url %></td>
    <td><%= banner.path %></td>
    <td><%= banner.parceiro_id %></td>
    <td><%= banner.data %></td>
    <td><%= banner.ativo %></td>
    <td><%= link_to 'Show', banner %></td>
    <td><%= link_to 'Edit', edit_banner_path(banner) %></td>
    <td><%= link_to 'Destroy', banner, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Banner', new_banner_path %>
